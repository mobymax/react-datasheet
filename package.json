{
  "name": "@moby/react-datasheet",
  "version": "1.0.19",
  "description": "Datasheet adaptation from nadbm/react-datasheet",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobymax/react-datasheet.git"
  },
  "author": "MobyMax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobymax/react-datasheet/issues"
  },
  "keywords": [
    "react-component",
    "react"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "std": "standard src/*.js",
    "stdfix": "standard src/*.js --fix",
    "clean": "rimraf dist",
    "build": "babel ./src  --out-dir ./lib && cp src/react-datasheet.css lib",
    "build:watch": "watch 'npm run build' ./src",
    "prepublish": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "standard src/*.js && NODE_ENV=test nyc --  mocha ./test/**/*.js --compilers js:babel-register",
    "test:watch": "watch 'npm run test' ./test ./src",
    "start": "webpack-dev-server --config ./webpack.config.js --history-api-fallback --port 3000 --host 0.0.0.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.23.4",
    "@babel/core": "~7.23.6",
    "@babel/polyfill": "~7.12.1",
    "@babel/plugin-proposal-class-properties": "~7.12.1",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.23.6",
    "@babel/preset-react": "~7.23.3",
    "@types/react": "^16.0.38",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "~6.8.1",
    "copy-webpack-plugin": "11.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "expect": "^1.20.2",
    "html-webpack-plugin": "~5.5.3",
    "jsdom": "^8.1.0",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^1.1.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "prop-types": "~15.8.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-addons-test-utils": "^15.0.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.3",
    "standard": "*",
    "style-loader": "~3.3.3",
    "watch": "^1.0.2",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "babel-loader": "~9.1.3"
  },
  "dependencies": {
    "clipboard-polyfill": "^2.5.4"
  },
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/index.js",
  "types": "types/react-datasheet.d.ts"
}
